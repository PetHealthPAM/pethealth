{
  "expo": {
    "name": "PetHealth",
    "slug": "PetHealth",
    "scheme": "PetHealth",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/pethealth.png",
      "resizeMode": "contain",
      "backgroundColor": "#7E57C2"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#7E57C2"
      },
      "package": "com.PetHeath"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
